{
  "name": "edev-uno",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:core": "vue-tsc -b && vite build && vite build --ssr src/entry-server.ts --outDir dist-ssr && node scripts/prerender.mjs",
    "build": "pnpm build:ssg:nojs",
    "build:ssg": "pnpm build:core",
    "build:ssg:nojs": "cross-env SSG_HYDRATE=false pnpm run build:core",
    "build:ssg:partial": "cross-env SSG_NOJS_ROUTES=/ pnpm run build:core",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.5.26",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "cross-env": "^10.1.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.5",
    "vue-tsc": "^3.2.2"
  },
  "packageManager": "pnpm@10.27.0"
}
